{
    "name": "robloach/templating-smarty",
    "type": "library",
    "description": "Symfony Templating Component support for Smarty.",
    "homepage": "http://github.com/RobLoach/templating-smarty",
    "license": "MIT",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "http://github.com/RobLoach/templating-smarty/graphs/contributors"
        }
    ],
    "support": {
        "issues": "http://github.com/RobLoach/templating-smarty/issues"
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/templating": "~2.6.0@beta",
        "smarty/smarty": "~3.1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "RobLoach\\TemplatingSmarty\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RobLoach\\TemplatingSmarty\\Test\\": "test/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
